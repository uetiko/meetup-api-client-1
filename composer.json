{
    "name": "javleds/meetup-api-client",
    "description": "PHP API wrapper for meetup.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Javier Ledezma",
            "email": "juls0593@gmail.com"
        },
        {
            "name": "Angel Barrientos",
            "email": "angel@uetiko.com"
        }
    ],
    "scripts": {
        "test": "phpunit",
        "phpcs": "php-cs-fixer fix -v --diff --dry-run",
        "phpcs-fix": "php-cs-fixer fix -v --diff"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Meetup\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meetup\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-json": "*",
        "psr/http-client": "^1.0",
        "psr/log": "^1.1",
        "league/fractal": "^0.19",
        "ramsey/uuid": "^4.0",
        "guzzlehttp/guzzle": "^6.5",
        "league/oauth2-client": "^2.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phploc/phploc": "^6.0",
        "phpmd/phpmd": "^2.8",
        "phpmetrics/phpmetrics": "^2.7",
        "squizlabs/php_codesniffer": "3.5.5"
    }
}
